<h1>Login</h1>

<form>
<form action="/auth/login" method="POST">
    <div class="form-group col-md-4 mb-3 align-items-center">
        <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
        <input type="email" class="form-control" id="inputEmail3" name="emailAddress" placeholder="Email" required>
    </div>
    <div class="form-group col-md-4 mb-3">
        <div id="pass"></div>
        <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>       
        <input type="password" class="form-control" id="inputPassword3" name="password" placeholder="Password" required>
    </div>
    <div class="form-group col-md-4 mb-3">
    <button class="btn btn-primary" type="submit">Login</button>
</div>
</form>
<div id="bk-sign"></div>
